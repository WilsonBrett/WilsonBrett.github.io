{
  "name": "brettwilsonrocks",
  "version": "1.0.0",
  "description": "Brett Wilson Home",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack && npm run build-styles",
